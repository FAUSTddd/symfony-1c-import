{
  "name": "faustddd/symfony-1c-import",
  "type": "symfony-bundle",
  "description": "Symfony 7 bundle for CommerceML (1C) catalog exchange",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/messenger": "^6.4|^7.0",
    "symfony/filesystem": "^6.4|^7.0",
    "ext-xmlreader": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "FaustDDD\\Symfony1cImport\\": "src/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^7.0"
    }
  },
  "archive": {
    "exclude": ["/.gitignore", "/phpunit.xml.dist", "/tests"]
  },
  "version": "1.0.0"
}