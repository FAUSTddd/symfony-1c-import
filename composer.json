{
  "name": "faustddd/symfony-1c-import",
  "type": "symfony-bundle",
  "description": "Symfony bundle for CommerceML (1C) catalog exchange",
  "keywords": ["symfony", "1c", "commerceml", "import", "catalog"],
  "license": "MIT",
  "authors": [
    {
      "name": "FAUSTddd",
      "homepage": "https://github.com/FAUSTddd"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-xmlreader": "*",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/messenger": "^6.4|^7.0",
    "symfony/filesystem": "^6.4|^7.0",
    "symfony/dependency-injection": "^6.4|^7.0",
    "symfony/config": "^6.4|^7.0",
    "symfony/routing": "^6.4|^7.0",
    "symfony/yaml": "^6.4|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "FaustDDD\\Symfony1cImport\\": "src/",
      "FaustDDD\\Symfony1cImport\\DependencyInjection\\": "src/DependencyInjection"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FaustDDD\\Symfony1cImport\\Tests\\": "tests/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^7.0"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}